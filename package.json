{
  "name": "parcel-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "parcel",
    "bootstrap",
    "sass",
    "javascript"
  ],
  "scripts": {
    "dev": "parcel ./src/index.html",
    "prebuild": "npx rimraf build",
    "build-dev": "parcel build --public-url ./ ./src/index.html   --no-minify  -d build",
    "build": "parcel build --public-url ./ ./src/index.html --no-minify --no-source-maps  -d build"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.0-beta3",
    "jquery": "^3.6.0",
    "randomcolor": "^0.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "sass": "^1.32.8"
  }
}
